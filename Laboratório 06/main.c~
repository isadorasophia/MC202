#include <stdio.h>
#include <stdlib.h>

// A partir da pre e in-ordem, gera a pos-ordem
void imprimePos(char *preOrdem, char *inOrdem, int tam);
int find (char key, char *vet, int tam);

int main () {
    int C, N;
    char *s1, *s2;
    
    // Numero de arvores a serem lidas
    scanf ("%d", &C);
    
    while (C--) {
        scanf("%d", &N);
        
        s1 = malloc ((N + 1) * sizeof(char));
        s2 = malloc ((N + 1) * sizeof(char));
        
        scanf("%s %s", s1, s2);
        
        imprimePos(s1, s2, N);
        printf("\n");
        
        free(s1);
        free(s2);
    }
    
    return 0;
}

void imprimePos(char *preOrdem, char *inOrdem, int tam) {
    int indexRaiz;
    
    // Encontra o index da raiz (sempre o primeiro elemento de pre-ordem) em
    // in-ordem
    indexRaiz = find(preOrdem[0], inOrdem, tam);
    
    /* Percorre a arvore da esquerda enquanto houver subarvore esquerda */
    if (indexRaiz != 0)
        imprimePos (preOrdem + 1, inOrdem, indexRaiz);
    
    /* Percorre a arvore da direita enquanto houver subarvore direita */
    if (indexRaiz != tam - 1)
        imprimePos (preOrdem + indexRaiz + 1, inOrdem + indexRaiz + 1, 
                    tam - indexRaiz - 1);
        
    printf("%c", preOrdem[0]);
}

int find (char key, char *vet, int tam) {
    int i;
    
    for (i = 0; i < tam; i++) {
        if (vet[i] == key)
            return i;
    }
}