#include <stdio.h>
#include <stdlib.h>
#include <string.h>

typedef struct Tree {
    int parent;
    int height;
} Tree;

void setFlorest (Tree *florest, int size);

void link (int A, int B, Tree *florest, int size);
void cut (int A, Tree *florest, int size);
int lca (int A, int B, Tree *florest, int max);